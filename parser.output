   37         | commands control
   38         | commands io ';'
   39         | %empty

   40 declare_var_local: declare
   41                  | declare TK_OC_LE TK_IDENTIFICADOR
   42                  | declare TK_OC_LE lit

   43 attribution: TK_IDENTIFICADOR '=' expression
   44            | TK_IDENTIFICADOR '[' expression ']' '=' expression

   45 expression: '(' expression ')'
   46           | expression '*' expression
   47           | expression '+' expression
   48           | expression '-' expression
   49           | expression '/' expression
   50           | expression TK_OC_EQ expression
   51           | lit
   52           | TK_IDENTIFICADOR
   53           | TK_IDENTIFICADOR '[' expression ']'
   54           | TK_IDENTIFICADOR params

   55 control: TK_PR_IF '(' expression ')' TK_PR_THEN block

   56 io: TK_PR_INPUT expression
   57   | TK_PR_OUTPUT list_exp

   58 list_exp: expression
   59         | expression ',' list_exp


